@model Assesment3.Controllers.StackOverFlowResponse

<div>
    @foreach (var q in Model.items)
    {
        <div class="card">
            <div class="card-header">
                Question
            </div>
            <div class="card-body">
                <blockquote class="blockquote mb-0">
                    <p>@q.title</p>
                    <footer class="blockquote-footer">Posted by: @q.owner.display_name <a asp-action="Question" asp-route-id="@q.question_id">View Question</a></footer>
                </blockquote>
            </div>
        </div>
    }
    @if (!Model.items.Any())
    {
        <div>No question found.</div>
    }

</div>